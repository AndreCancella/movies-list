{
    "version": 2,
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "src/main.ts"
      }
    ],
    "env": {
        "JWT_SECRET": "your_secret_key_here",
        "JWT_EXPIRATION_TIME": "3600"
    }
  }
  